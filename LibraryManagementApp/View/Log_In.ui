	border-radius: 10px;
	padding: 5px 10px;
	font-family: &quot;Montserrat Medium&quot;;
	font-size: 15px;
}

#widgetLogIn QLineEdit:focus {
	border-color: #0A66C2;
}

/*Set style for Log In button*/
#btnLogIn {
	background-color: #0A66C2;
	font-family: &quot;Montserrat SemiBold&quot;;
	font-size: 20px;
	border-radius: 10px;
	color: rgb(255, 255, 255);
}

#btnLogIn:pressed {
	padding-left: 15px;
}

#btnLogIn:hover, #btnLogIn:pressed {
	background-color: rgb(8, 88, 168);
}</string>
